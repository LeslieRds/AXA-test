{
  "name": "rest-api-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "~14"
  },
  "scripts": {
    "start": "webpack && react-scripts start",
    "build": "webpack && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "webpack --watch --mode development",
    "dev": "nodemon --exec 'node -r @babel/register' index.js",
    "start-api": "json-server ./api.js",
    "delay-api": "json-server ./api.js --delay 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-Decoen/rest-api-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Julien-Decoen/rest-api-test/issues"
  },
  "homepage": "https://github.com/Julien-Decoen/rest-api-test#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.18.6",
    "axios": "^1.6.7",
    "chart.js": "^4.4.1",
    "d3": "^7.8.5",
    "express": "^4.16.4",
    "json-server": "^0.17.0",
    "node-sass": "^9.0.0",
    "nunjucks": "^3.1.3",
    "path": "^0.12.7",
    "postcss-loader": "^8.1.0",
    "react-chartjs-2": "^5.2.0",
    "react-table": "^7.8.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/register": "^7.23.7",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.0",
    "nodemon": "^2.0.18",
    "sass-loader": "^14.1.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^5.0.2"
  },
  "nodemonConfig": {
    "dev": "nodemon -r @babel/register index.js",
    "restartable": "rs",
    "ignore": [
      ".git/*",
      "node_modules/*",
      ".public/*"
    ],
    "verbose": true,
    "execMap": {
      "js": "node -r @babel/register"
    },
    "events": {
      "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
    },
    "watch": [
      "app/*",
      "front/*"
    ],
    "ext": "js json html css"
  }
}
